{
    "$schema": "https://hypar.io/Schemas/Function.json",
    "id": "31ec3b95-5062-47b9-a1e0-e3550bf7e2d1",
    "name": "Floors by Sketch",
    "description": "Create floors by drawing them manually.",
    "language": "C#",
    "model_dependencies": [],
    "model_output": "Floors",
    "input_schema": {
        "type": "object",
        "properties": {
            "Floors": {
                "description": "Draw a list of floor profiles",
                "default": [],
                "type": "array",
                "items": {
                    "$hyparDisplayName": "Floor",
                    "type": "object",
                    "$hyparStyle": "row",
                    "required": [
                        "Boundary",
                        "Elevation",
                        "Thickness"
                    ],
                    "properties": {
                        "Boundary": {
                            "$ref": "https://hypar.io/Schemas/Geometry/Polygon.json"
                        },
                        "Elevation": {
                            "type": "number",
                            "description": "The elevation of the floor",
                            "$hyparUnitType": "length",
                            "default": 0
                        },
                        "Thickness": {
                            "type": "number",
                            "description": "The thickness of the floor",
                            "$hyparUnitType": "length",
                            "default": 0.3
                        }
                    }
                },
                "minItems": 1
            }
        }
    },
    "outputs": [
        {
            "unit_type": "area",
            "name": "Area",
            "description": "The total floor area",
            "type": "number"
        }
    ],
    "repository_url": "",
    "source_file_key": null,
    "preview_image": null
}